{
  "expo": {
    "name": "maozinhagamer",
    "slug": "maozinhagamer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "maozinhagamer",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2E0000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.maozinhagamer.maozinhagamerapp"
    },
    "platforms": ["ios", "android"],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.maozinhagamer.maozinhagamerapp"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#2E0000"
    },
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "userFrameworks": "static"
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b0115e21-68ee-4dab-b3d3-8ae6ae1a8c4f"
      }
    }
  }
}
