{
  "expo": {
    "name": "maozinhagamer",
    "slug": "maozinhagamer",
    "version": "1.0.1",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "maozinhagamer",
    "splash": {
      "layout": {
        "backgroundColor": "#2E0000",
        "content": {
          "image": "./assets/splash.png",
          "resizeMode": "contain"
        },
        "statusBarStyle": "light"
      },
      "backgroundColor": "#2E0000"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icon.png",
      "bundleIdentifier": "com.maozinhagamer.maozinhagamerapp",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "platforms": ["ios", "android"],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2E0000"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.maozinhagamer.maozinhagamerapp",
      "googleServicesFile": "./google-services.json"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#2E0000",
      "translucent": false
    },
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "userFrameworks": "static"
          },
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33
          }
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b0115e21-68ee-4dab-b3d3-8ae6ae1a8c4f"
      }
    }
  }
}
