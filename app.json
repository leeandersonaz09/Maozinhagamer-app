{
  "expo": {
    "name": "maozinhagamer",
    "slug": "maozinhagamer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "maozinhagamer",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#8F0000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.leeandersonaz09.maozinhagamer"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#2E0000"
    },
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b0115e21-68ee-4dab-b3d3-8ae6ae1a8c4f"
      }
    }
  }
}
